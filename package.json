{
  "name": "@shifter/cli",
  "description": "Simple Shifter management cli",
  "version": "0.2.0",
  "author": "Shifter",
  "bin": {
    "shifter": "./bin/run"
  },
  "bugs": "https://github.com/getshifter/cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "axios": "^0.21.1",
    "cli-ux": "^5.4.9",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@oclif/dev-cli": "1.26.0",
    "@types/node": "14.17.5",
    "eslint": "7.30.0",
    "eslint-config-oclif": "3.1.0",
    "eslint-config-oclif-typescript": "0.2.0",
    "globby": "11.0.4",
    "husky": "7.0.1",
    "lint-staged": "11.0.0",
    "semantic-release": "17.4.4",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/getshifter/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "shifter",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "getshifter/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "lint": "eslint src/**/*.ts --fix",
    "version": "oclif-dev readme && git add README.md",
    "semantic-release": "semantic-release"
  },
  "types": "lib/index.d.ts"
}
